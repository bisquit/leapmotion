package  {	import flash.display.Sprite;	import leap.LeapInit;	import flash.events.Event;	import flash.display.Stage;	import leap.LeapInterface;	import objects.Player;		public class Main extends Sprite{				public var player:Player;		public var mLeap:LeapInit;		private var _stage:Stage;				public function Main(stage) {						_stage = stage;						player = new Player();			player.x = 0;			player.y = 0;			_stage.addChild(player);			LeapInterface.addPlayer(player);						mLeap = new LeapInit(_stage, player.width, player.height);			_stage.addEventListener(Event.ENTER_FRAME, onFrame);					}				private function onFrame(e:Event):void{						player.x = LeapInterface.Sx;			player.y = LeapInterface.Sy;					}	}	}
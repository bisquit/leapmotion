package phase {	import flash.display.Sprite;	import flash.geom.Point;	import objects.Logo;	import core.Mediator;		public class EndPhase extends Sprite implements IPhase{				public function EndPhase() {			var logo:Logo = new Logo();			logo.x = Mediator.stageWidth / 2;			logo.y = Mediator.stageHeight / 2;			addChild(logo);		}				public function start():void {			trace("end start");			Mediator._stage.addChild(this);			Mediator._stage.setChildIndex(Mediator._player, Mediator._stage.numChildren - 1);					}				public function afterCatch(info:Object = null):void {					}				public function destroy():void {					}	}	}
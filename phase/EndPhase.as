package phase {	import flash.display.Sprite;	import flash.geom.Point;	import core.Mediator;	import leap.LeapSystem;	import objects.Catchable;	import core.SoundManager;		public class EndPhase extends Sprite implements IPhase{				public function EndPhase() {			var logo:Catchable = new Catchable("logo");			logo.x = Mediator.stageWidth / 2;			logo.y = Mediator.stageHeight / 2;//			addChild(logo);		}				public function start():void {			trace("end start");			Mediator._stage.addChild(this);			Mediator._stage.setChildIndex(Mediator._player, Mediator._stage.numChildren - 1);			this.x = 500;			this.y = 100;			LeapSystem.initialize();			Mediator.initialize();						Mediator._black.fadeLight();						SoundManager.play("ending", true, 1);						Mediator.cr.takeRanking();					}				public function afterCatch(info:Object = null):void {					}				public function destroy():void {					}	}	}
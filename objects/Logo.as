package objects{	import flash.display.Bitmap;	import flash.display.Sprite;	public class Logo extends Sprite {		[Embed(source="../assets/images/logo.png")]		private var LogoImage:Class;				public var vx:Number = 0;		public var vy:Number = 0;		public var acceleration:Number = 0;		public var point:Number = 0;		public var delete_flag:Boolean = false;		public function Logo() {						var logo_img:Bitmap = new LogoImage();			logo_img.scaleX = logo_img.scaleY = 0.5;			logo_img.x = - logo_img.width/2;			logo_img.y = - logo_img.height/2;			addChild(logo_img);		}	}}
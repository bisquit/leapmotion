package objects{	import flash.display.Bitmap;	import flash.display.Sprite;	import flash.events.Event;		public class UFO extends Sprite	{		[Embed(source="../assets/images/ufo01.png")]		private var UFO01Img:Class;		[Embed(source="../assets/images/ufo01_anim.png")]		private var UFO01AnimImg:Class;				[Embed(source="../assets/images/ufo02.png")]		private var UFO02Img:Class;		[Embed(source="../assets/images/ufo02_anim.png")]		private var UFO02AnimImg:Class;				[Embed(source="../assets/images/ufo03.png")]		private var UFO03Img:Class;		[Embed(source="../assets/images/ufo03_anim.png")]		private var UFO03AnimImg:Class;				[Embed(source="../assets/images/ufo04.png")]		private var UFO04Img:Class;		[Embed(source="../assets/images/ufo04_anim.png")]		private var UFO04AnimImg:Class;				[Embed(source="../assets/images/ufo05.png")]		private var UFO05Img:Class;		[Embed(source="../assets/images/ufo05_anim.png")]		private var UFO05AnimImg:Class;				private var ufo_img:Bitmap;		private var ufo_anim:Bitmap;		private var count:int = 0;				public var vx:Number;		public var vy:Number;		public var vx_max:Number = 5;		public var vx_min:Number = -5;		public var vy_max:Number = 0;		public var vy_min:Number = -3;		public var acceleration:Number = 0;		public var point:Number = -10;		public var delete_flag:Boolean = false;		public var isCollided:Boolean = false;				public function UFO(W:Number, H:Number)		{			var rand_id:Number = Math.random();						if(rand_id <= 0.2){				ufo_img = new UFO01Img();				ufo_anim = new UFO01AnimImg();				ufo_img.scaleX = ufo_img.scaleY = ufo_anim.scaleX = ufo_anim.scaleY = 0.9;				addChild(ufo_img);				addChild(ufo_anim);			} else if(rand_id <= 0.4){				ufo_img = new UFO02Img();				ufo_anim = new UFO02AnimImg();				ufo_img.scaleX = ufo_img.scaleY = ufo_anim.scaleX = ufo_anim.scaleY = 0.9;				addChild(ufo_img);				addChild(ufo_anim);			} else if(rand_id <= 0.6){				ufo_img = new UFO03Img();				ufo_anim = new UFO03AnimImg();				ufo_img.scaleX = ufo_img.scaleY = ufo_anim.scaleX = ufo_anim.scaleY = 0.9;				addChild(ufo_img);				addChild(ufo_anim);			} else if(rand_id <= 0.8){				ufo_img = new UFO04Img();				ufo_anim = new UFO04AnimImg();				ufo_img.scaleX = ufo_img.scaleY = ufo_anim.scaleX = ufo_anim.scaleY = 0.9;				addChild(ufo_img);				addChild(ufo_anim);			} else {				ufo_img = new UFO05Img();				ufo_anim = new UFO05AnimImg();				ufo_img.scaleX = ufo_img.scaleY = ufo_anim.scaleX = ufo_anim.scaleY = 0.9;				addChild(ufo_anim);				addChild(ufo_img);			} 						ufo_img.x = ufo_anim.x = -ufo_img.width/2;			ufo_img.y = ufo_anim.y = -ufo_img.height/2;									var x_min:Number = W / 3 + ufo_img.width / 2;			var x_max:Number = W - ufo_img.width / 2;			var y_min:Number = 0;			var y_max:Number = 0;						this.x = int( Math.random() * (x_max - x_min + 1) ) + x_min;			this.y = int( Math.random() * (y_max - y_min + 1) ) + y_min;						vx_min = -3;			vx_max = -1;			vy_min = 3;			vy_max = 3;						this.vx = int( Math.random() * (vx_max - vx_min + 1) ) + vx_min;			this.vy = int( Math.random() * (vy_max - vy_min + 1) ) + vy_min;						this.name = "ufo";						addEventListener(Event.ENTER_FRAME, onFrame);		}				private function onFrame(e:Event):void{			if(count%80 == 0){				ufo_anim.visible = false;			} else if(count%40 == 0){				ufo_anim.visible = true;			}			count++;		}				public function destroy():void {			removeChild(ufo_img);			ufo_img = null;		}	}}
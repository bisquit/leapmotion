package objects{	import flash.display.Bitmap;	import flash.display.Sprite;	public class StartButton extends Sprite {		[Embed(source="../assets/images/logo.png")]		private var LogoImage:Class;				private var btn_img:Bitmap;				public var vx:Number = 0;		public var vy:Number = 0;		public var acceleration:Number = 0;		public var point:Number = 0;		public var delete_flag:Boolean = false;		public var isCollided:Boolean = false;		[Embed(source="../assets/images/start-btn.png")]		private var StartImg:Class;		public function StartButton() {						btn_img = new StartImg();			btn_img.scaleX = btn_img.scaleY = 0.8;			btn_img.x = - btn_img.width/2;			btn_img.y = - btn_img.height/2;			addChild(btn_img);		}				public function destroy():void {			removeChildren();			btn_img = null;		}	}}
package core{		import flash.display.Sprite;	import flash.display.StageAlign;	import flash.display.StageScaleMode;	import leap.LeapInit;		[SWF(width = 1000, height = 750, frameRate = "60", backgroundColor = "#000000")]	public class Entry extends Sprite {				private var mLeap:LeapInit;				public function Entry() {						stage.align = StageAlign.TOP_LEFT;			stage.scaleMode = StageScaleMode.NO_SCALE;						// Mediatorにステージを登録			Mediator.registerStage(stage);						// Starling 開始			new Starling_test02(stage);						// Leap motion セットアップ			mLeap = new LeapInit(stage);											}	}	}